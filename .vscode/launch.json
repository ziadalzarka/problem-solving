  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "majority-element",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "largest-rectangle-area",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=${workspaceFolder}/largest-rectangle-area/Cargo.toml"
        ]
      },
      "args": []
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "task-scheduler",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=${workspaceFolder}/task-scheduler/Cargo.toml"
        ]
      },
      "args": []
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "search-2d-matrix",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=${workspaceFolder}/search-2d-matrix/Cargo.toml"
        ]
      },
      "args": []
    },
    {
      "name": "trapping-rain-water",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    }
  ]
}
